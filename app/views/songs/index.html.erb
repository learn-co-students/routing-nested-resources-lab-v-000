<ul>
<% @songs.each do |song| %>
  <%if song.artist%>
    <li><%= link_to display_artist(song), artist_songs_path(song.artist) %> - <%= link_to song.title, song_path(song) %></li>
  <%else%>
    <li><%= link_to "Add Artist", edit_song_path(song) %> - <%= link_to song.title, song_path(song) %></li>
  <%end%>
<% end %>
</ul>
<br>
<%= link_to "Add New Song", new_song_path %>
